{
  "name": "intel-parsely",
  "version": "3.0.0",
  "description": "Boilerplate for node projects",
  "main": "dist/source/index.js",
  "scripts": {
    "test:dev": "npm run postversion && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "eslint": "eslint ./",
    "postversion": "./bin/build.sh",
    "cover:dev": "npm run cover -- --reporter=html --reporter=lcov",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t"
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "pre-commit": [
    "eslint",
    "test:dev"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.intel.com:IML/parsely.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-plugin-__coverage__": "0.111111.11",
    "babel-plugin-transform-class-properties": "6.8.0",
    "babel-plugin-transform-flow-strip-types": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-plugin-transform-strict-mode": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "eslint": "2.9.0",
    "eslint-plugin-flow-vars": "0.4.0",
    "eslint-plugin-flowtype": "2.2.7",
    "intel-deep-freeze": "1.0.1",
    "intel-jasmine-junit-reporter": "1.0.0",
    "intel-jasmine-n-matchers": "2.1.1",
    "jasmine": "2.4.1",
    "nyc": "6.4.4",
    "pre-commit": "1.1.3"
  },
  "dependencies": {
    "intel-fp": "4.3.0"
  },
  "nyc": {
    "include": [
      "/"
    ],
    "exclude": [
      "test"
    ]
  }
}
