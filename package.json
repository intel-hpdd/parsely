{
  "name": "intel-parsely",
  "version": "2.1.1",
  "description": "Boilerplate for node projects",
  "main": "dist/source/index.js",
  "scripts": {
    "test:dev": "npm run postversion && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "eslint": "eslint ./",
    "jscs": "jscs .",
    "postversion": "./bin/build.sh",
    "cover:dev": "npm run cover -- --reporter=html --reporter=lcov",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t"
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "pre-commit": [
    "jscs",
    "eslint",
    "test:dev"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.intel.com:IML/parsely.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.3",
    "babel-plugin-__coverage__": "0.111111.1",
    "babel-plugin-transform-flow-strip-types": "6.7.0",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-plugin-transform-strict-mode": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "eslint": "2.8.0",
    "eslint-plugin-flow-vars": "0.3.0",
    "intel-jasmine-junit-reporter": "1.0.0",
    "intel-jasmine-n-matchers": "2.1.1",
    "jasmine": "2.4.1",
    "jscs": "2.11.0",
    "nyc": "6.4.0",
    "pre-commit": "1.1.2"
  },
  "dependencies": {
    "intel-fp": "4.2.3"
  },
  "nyc": {
    "include": [
      "/"
    ],
    "exclude": [
      "test"
    ]
  }
}
